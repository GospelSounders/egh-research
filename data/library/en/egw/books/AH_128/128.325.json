  {
    "para_id": "128.329",
    "id_prev": "128.328",
    "id_next": "128.330",
    "refcode_1": "AH",
    "refcode_2": "79",
    "refcode_3": "2",
    "refcode_4": "",
    "refcode_short": "AH 79.2",
    "refcode_long": "The Adventist Home, p. 79.2 (Ellen Gould White)",
    "element_type": "p",
    "element_subtype": "standard-indented",
    "content": "Boys and girls enter upon the marriage relation with unripe love, immature judgment, without noble, elevated feelings, and take upon themselves the marriage vows, wholly led by their boyish, girlish passions....",
    "puborder": 210,
    "translations": [
      {
        "para_id": "13839.254",
        "lang": "bg",
        "refcode": "ХД 30.5"
      },
