        "lang": "es",
        "refcode": "HC 431.4"
      },
      {
        "para_id": "193.2200",
        "lang": "fr",
        "refcode": "FC 460.3"
      },
      {
        "para_id": "12869.2787",
        "lang": "id",
        "refcode": "MKB 454.1"
      },
      {
        "para_id": "1955.2853",
        "lang": "pt",
        "refcode": "LA 474.4"
      },
      {
        "para_id": "2026.2218",
