  {
    "para_id": "128.435",
    "id_prev": "128.434",
    "id_next": "128.436",
    "refcode_1": "AH",
    "refcode_2": "102",
    "refcode_3": "2",
    "refcode_4": "",
    "refcode_short": "AH 102.2",
    "refcode_long": "The Adventist Home, p. 102.2 (Ellen Gould White)",
    "element_type": "p",
    "element_subtype": "standard-indented",
    "content": "God wants the home to be the happiest place on earth, the very symbol of the home in heaven. Bearing the marriage responsibilities in the home, linking their interests with Jesus Christ, leaning upon His arm and His assurance, husband and wife may share a happiness in this union that angels of God commend.",
    "puborder": 280,
